          OutlineColor: #e8ec1a(0.5);
          OutlineSize: 1.5;

          Label  {
            Text: "System Usage";
            Style: (
              FontSize: 16,
              TextColor: #93844c,
              RenderBold: true
            );
          }

          Group {
            LayoutMode: Top;
            Anchor: (Top: 5);

            Group {
              Anchor: (Height: 8, Bottom: 8);
              Background: "../../Common/ProgressBar.png";

              ProgressBar #SystemCPUUsageBar {
                BarTexturePath: "../../Common/ProgressBarFill.png";
                EffectTexturePath: "../../Common/ProgressBarEffect.png";
                EffectWidth: 102;
                EffectHeight: 58;
                EffectOffset: 74;
                Value: 0.1;
              }
            }

            Group {
              LayoutMode: Left;
              
              Label {FlexWeight: 1;}
              Label #SystemCPUUsage {
                Text: "45%";
                Style: (
                  FontSize: 14,
                  TextColor: #ffffff,
                  RenderBold: true
                );
              }
              Label {FlexWeight: 1;}
            }
