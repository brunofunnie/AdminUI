    Anchor: (Width: 600, Height: 669);


    #Title {
      Group {
        TextButton #BackButton {
          Anchor: (Left: 12, Top: 4, Width: 64, Height: 24);
          Padding: (Left: 8, Top: 2);
          Text: "< Back";
          Style: TextButtonStyle(
            Default: (LabelStyle: (FontSize: 14, TextColor: #93844c, RenderBold: true)),
            Sounds: $C.@ButtonSounds,
          );
          Background: #000000(0.13);
        }
        $C.@Title {
          @Text = "SERVER STATS";
        }
      }
    }

    #Content {
      LayoutMode: Top;

      Group {
        LayoutMode: Left;
        Anchor: (Bottom: 4);

        Background: #93844c(0.13);

        Label {FlexWeight: 1;}

        Label {
          Text: "CPU";
          Style: (
            FontSize: 22,
            TextColor: #93844c,
             RenderBold: true
          );
        }

        Label {FlexWeight: 1;}

      }

